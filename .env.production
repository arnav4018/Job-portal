# Production environment variables
# These are fallback values for build time

# Database (will be overridden by Vercel environment variables)
DATABASE_URL="file:./build.db"

# Auth (will be overridden by Vercel environment variables)
NEXTAUTH_SECRET="build-time-secret"
NEXTAUTH_URL="http://localhost:3000"

# Build flags
SKIP_ENV_VALIDATION="true"
SKIP_DATABASE_VALIDATION="true"